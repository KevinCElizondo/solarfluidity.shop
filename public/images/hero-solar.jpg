<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="1200" height="800" viewBox="0 0 1200 800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#4A90E2;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#87CEFA;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="1200" height="800" fill="url(#skyGradient)"/>
  <rect x="0" y="600" width="1200" height="200" fill="#8BC34A"/>
  
  <!-- Sun -->
  <circle cx="900" cy="150" r="80" fill="#FDB813"/>
  
  <!-- Solar Panels -->
  <g transform="translate(100, 400)">
    <rect x="0" y="0" width="300" height="200" fill="#1A237E" stroke="#424242" stroke-width="5"/>
    <line x1="0" y1="50" x2="300" y2="50" stroke="#424242" stroke-width="2"/>
    <line x1="0" y1="100" x2="300" y2="100" stroke="#424242" stroke-width="2"/>
    <line x1="0" y1="150" y2="150" x2="300" stroke="#424242" stroke-width="2"/>
    <line x1="75" y1="0" x2="75" y2="200" stroke="#424242" stroke-width="2"/>
    <line x1="150" y1="0" x2="150" y2="200" stroke="#424242" stroke-width="2"/>
    <line x1="225" y1="0" x2="225" y2="200" stroke="#424242" stroke-width="2"/>
    <rect x="-5" y="200" width="310" height="10" fill="#795548"/>
    <polygon points="150,210 120,250 180,250" fill="#795548"/>
  </g>
  
  <g transform="translate(500, 450)">
    <rect x="0" y="0" width="300" height="200" fill="#1A237E" stroke="#424242" stroke-width="5"/>
    <line x1="0" y1="50" x2="300" y2="50" stroke="#424242" stroke-width="2"/>
    <line x1="0" y1="100" x2="300" y2="100" stroke="#424242" stroke-width="2"/>
    <line x1="0" y1="150" y2="150" x2="300" stroke="#424242" stroke-width="2"/>
    <line x1="75" y1="0" x2="75" y2="200" stroke="#424242" stroke-width="2"/>
    <line x1="150" y1="0" x2="150" y2="200" stroke="#424242" stroke-width="2"/>
    <line x1="225" y1="0" x2="225" y2="200" stroke="#424242" stroke-width="2"/>
    <rect x="-5" y="200" width="310" height="10" fill="#795548"/>
    <polygon points="150,210 120,250 180,250" fill="#795548"/>
  </g>
  
  <g transform="translate(900, 500)">
    <rect x="0" y="0" width="200" height="150" fill="#1A237E" stroke="#424242" stroke-width="5"/>
    <line x1="0" y1="50" x2="200" y2="50" stroke="#424242" stroke-width="2"/>
    <line x1="0" y1="100" x2="200" y2="100" stroke="#424242" stroke-width="2"/>
    <line x1="50" y1="0" x2="50" y2="150" stroke="#424242" stroke-width="2"/>
    <line x1="100" y1="0" x2="100" y2="150" stroke="#424242" stroke-width="2"/>
    <line x1="150" y1="0" x2="150" y2="150" stroke="#424242" stroke-width="2"/>
    <rect x="-5" y="150" width="210" height="10" fill="#795548"/>
    <polygon points="100,160 70,200 130,200" fill="#795548"/>
  </g>
  
  <!-- Clouds -->
  <g fill="white" opacity="0.8">
    <ellipse cx="200" cy="120" rx="60" ry="40"/>
    <ellipse cx="250" cy="120" rx="70" ry="50"/>
    <ellipse cx="300" cy="120" rx="60" ry="40"/>
    
    <ellipse cx="600" cy="100" rx="70" ry="40"/>
    <ellipse cx="650" cy="100" rx="80" ry="50"/>
    <ellipse cx="700" cy="100" rx="70" ry="40"/>
  </g>
</svg>
